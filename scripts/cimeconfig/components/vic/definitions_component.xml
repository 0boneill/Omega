<?xml version="1.0"?>

<?xml-stylesheet type="text/xsl" href="definitions_variables" ?>

<definitions_variables>

  <entry id="COMP_LND"> 
    <type>char</type>
    <valid_values>vic</valid_values>
    <default_value>vic</default_value>
    <group>case_comp</group>
    <file>env_case.xml</file>
    <desc>Name of land component</desc>
  </entry>

  <entry id="VIC_CONFIG_OPTS">
    <type>char</type>
    <valid_values></valid_values>
    <default_value></default_value>
    <group>build_component_vic</group>
    <file>env_build.xml</file>
    <desc>VIC configure options</desc>
  </entry> 

  <entry id="VIC_NML_USE_CASE" >
    <type>char</type>
    <valid_values></valid_values>
    <default_value>UNSET</default_value>
    <group>run_component_vic</group>
    <desc>VIC namelist use_case</desc>
  </entry>

  <entry id="VIC_NAMELIST_OPTS">
    <type>char</type>
    <valid_values></valid_values>
    <default_value></default_value>
    <group>run_component_vic</group>
    <file>env_run.xml</file>
    <desc>VIC specific namelist settings</desc>
  </entry>

  <entry id="VIC_CO2_TYPE"> 
    <type>char</type>
    <valid_values>constant,diagnostic,prognostic</valid_values>
    <list>1</list>
    <default_value>constant</default_value>
    <group>run_component_vic</group>
    <file>env_run.xml</file>
    <desc>vic co2 type"</desc>
  </entry>

  <entry id="VIC_FORCE_COLDSTART">
    <type>char</type>
    <valid_values>on,off</valid_values>
    <default_value>on</default_value>
    <group>run_component_vic</group>
    <file>env_run.xml</file>
    <desc>Force VIC model to do a cold-start</desc>
  </entry>

  <entry id="VIC_PT1_NAME"> 
    <type>char</type>
    <valid_values></valid_values>
    <default_value>UNSET</default_value>
    <group>run_component_vic</group>
    <file>env_run.xml</file>
    <desc>VIC Single point name</desc>
  </entry>

  <entry id="VIC_USRDAT_NAME"> 
    <type>char</type>
    <valid_values></valid_values>
    <default_value>UNSET</default_value>
    <group>run_component_vic</group>
    <file>env_run.xml</file>
    <desc>My Data ID</desc>
  </entry> 

</definitions_variables>

