<?xml version="1.0"?>

<config_pio>

  <!-- always place default value first without any attributes -->
  <!-- then place the attribute for grid or compset afterwards -->

  <mach name="yellowstone">
    <PIO_TYPENAME>netcdf</PIO_TYPENAME> 
    <PIO_TYPENAME grid="l%0.9">pnetcdf</PIO_TYPENAME>
    <PIO_TYPENAME grid="l%.+cru">pnetcdf</PIO_TYPENAME>
    <PIO_TYPENAME grid="l%ne30">pnetcdf</PIO_TYPENAME>
    
    <PIO_STRIDE>-1</PIO_STRIDE> 
    <PIO_STRIDE grid="l%ne30">15</PIO_STRIDE>
  </mach>

</config_pio>
